# Autorestic exclude patterns
# Generated by Ansible - do not edit manually

# Cache directories
.cache/
**/.cache/

# Build artifacts
**/node_modules/
**/target/
**/dist/
**/build/
**/.next/
**/.nuxt/

# Temporary files
**/*.tmp
**/*.temp
**/.tmp/
**/tmp/

# Downloads
Downloads/
**/Downloads/

# Large media files (optional)
{% if restic.exclude_large_media %}
**/*.iso
**/*.dmg
**/*.ova
{% endif %}

# System files
**/.Trash/
**/.DS_Store
**/Thumbs.db

# Application caches
**/.npm/
**/.yarn/
**/.cargo/registry/
**/.cargo/git/
**/.rustup/toolchains/

# Virtual environments
**/venv/
**/.venv/
**/env/

# IDE files
**/.idea/
**/.vscode/
**/__pycache__/

# Custom exclusions
{% for exclude in restic.custom_excludes %}
{{ exclude }}
{% endfor %}
